# FisioRAG - Example environment
APP_ENV=development
APP_HOST=0.0.0.0
APP_PORT=8000
LOG_LEVEL=INFO
ENABLE_METRICS=true

# PostgreSQL / pgvector
POSTGRES_DB=rag_db
POSTGRES_USER=rag_user
POSTGRES_PASSWORD=rag_password
DATABASE_URL=postgresql+asyncpg://rag_user:rag_password@postgres:5432/rag_db

# Neo4j
NEO4J_USER=neo4j
NEO4J_PASSWORD=password
NEO4J_URI=bolt://neo4j:7687

# Redis
REDIS_URL=redis://redis:6379/0

# OpenAI (optional for local)
OPENAI_API_KEY=

# Misc
METRICS_PORT=9090
